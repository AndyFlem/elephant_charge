<%= form_tag({action: :uploadlogo}, multipart: true) do %>
    <div class="form-group row">
      <label class="col-sm-2 col-form-label" for="beneficiary_Geography">Logo</label>
      <div class="col-sm-4">
        <%= file_field_tag 'logo',class:"form-control"  %>
      </div>
      <div class="col-sm-4">
        <%= image_tag @beneficiary.logo.url(:medium) %>
      </div>
    </div>
    <%= submit_tag "Upload", class: "btn btn-primary" %>
<% end %>