<div class="heading-bar"><h3>Cars</h3></div>

<%= render 'form' %>

<div class="row">
  <% @photos.each_with_index do |photo,i| %>
      <% if i%4==0 %>
          </div>
          <div class="row">
      <% end %>
      <div class="col-sm-3">
        <div class="card">
          <%= image_tag photo.photo.url(:medium),class: "card-img-top",style:"display:block;margin-left:auto;margin-right:auto;margin-top:10px;" %>
          
        </div>
      </div>
  <% end %>
  </div>